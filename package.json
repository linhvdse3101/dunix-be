{
  "name": "dunix-be",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:dev": "ts-node-dev --transpile-only src/index.ts",
    "migrate": "ts-node -r tsconfig-paths/register src/migrate.ts",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.883.0",
    "@aws-sdk/s3-request-presigner": "^3.883.0",
    "@loopback/authentication": "^12.0.5",
    "@loopback/boot": "^8.0.4",
    "@loopback/core": "^7.0.3",
    "@loopback/repository": "^8.0.3",
    "@loopback/rest": "^15.0.4",
    "@loopback/rest-explorer": "^8.0.4",
    "@loopback/security": "^0.12.4",
    "@loopback/service-proxy": "^8.0.3",
    "@types/node": "^24.3.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.1",
    "jsonwebtoken": "^9.0.2",
    "loopback": "^3.28.0",
    "loopback-connector-mysql": "^8.0.0",
    "mime-types": "^3.0.1",
    "node-pre-gyp": "^0.17.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "uuid": "^12.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.2"
  }
